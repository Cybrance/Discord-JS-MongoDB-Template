# Copy this file to .env and replace placeholders with real values. Do NOT commit real secrets.

# Discord bot token (from https://discord.com/developers)
token=YOUR_DISCORD_BOT_TOKEN_HERE

# Bot client ID
CLIENT_ID=YOUR_CLIENT_ID_HERE

# MongoDB connection string (e.g. MongoDB Atlas)
# Example: mongodb+srv://username:password@cluster0.mongodb.net/dbname?retryWrites=true&w=majority
MONGODBURI=YOUR_MONGODB_CONNECTION_STRING_HERE

# Optional: runtime environment
NODE_ENV=development

# Optional: command prefix for non-slash commands
PREFIX=!
